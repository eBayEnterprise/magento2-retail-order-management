{
    "name": "ebayenterprise/magento2-address-validation",
    "description": "Magento 2 module for validating addresses using the eBay Enterprise Retail Order Management API.",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Scott van Brug",
            "email": "svanbrug@ebay.com"
        }
    ],
    "minimum-stability": "dev",
    "type": "magento2-module",
    "prefer-stable": true,
    "require": {
        "php": "~5.5.0|~5.6.0",
        "ebayenterprise/retail-order-management": "~1.1",
        "magento/framework": "~0.0",
        "magento/module-customer": "~0.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "pdepend/pdepend": "~1.1",
        "phpdocumentor/phpdocumentor": "~2.4",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~1.5",
        "phpunit/phpunit": "~4.5",
        "sebastian/phpcpd": "~2.0",
        "squizlabs/php_codesniffer": "~1.5",
        "magento/product-community-edition": "~0.0"
    },
    "repositories": {
        "retail-order-management": {
            "type": "vcs",
            "url": "https://github.com/eBayEnterprise/RetailOrderManagement-SDK.git"
        },
        "magento": {
            "type": "composer",
            "url": "https://packages.magento.com"
        }
    },
    "extra": {
        "map": [
            [
                "src/EbayEnterprise/Address/*",
                "EbayEnterprise/Address"
            ]
        ],
        "magento-root-dir": "build/magento",
        "magento-force": true
    },
    "autoload": {
        "psr-4": {
            "EbayEnterprise\\Address\\": ["src/EbayEnterprise/Address", "tests/EbayEnterprise/Address"],
            "Magento\\Setup\\": "vendor/magento/magento2-base/setup/src/Magento/Setup"
        }
    }
}
